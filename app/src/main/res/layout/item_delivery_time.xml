<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data class="DeliveryTimeAdapterBinding">

        <variable
            name="data"
            type="com.iitms.bfr.data.model.DeliveryTime" />

    </data>

    <LinearLayout
        android:layout_margin="@dimen/dp_05"
        android:id="@+id/cv_main"
        android:background="@drawable/rounded_background_half_circle_white"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/tv_time_name"
            android:padding="@dimen/dp_10"
            android:text="8 Am to pm"
            android:textColor="@color/divider"
            style="@style/CustomTextView"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </LinearLayout>

</layout>
